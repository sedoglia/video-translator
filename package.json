{
  "name": "video-translator",
  "version": "1.0.0",
  "description": "Desktop app for translating video audio using AI",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "npm run build && concurrently \"npm run watch\" \"npm run electron:dev\"",
    "watch": "webpack --config webpack.config.js --watch",
    "build": "webpack --config webpack.config.js",
    "electron": "electron .",
    "electron:dev": "wait-on dist/main.js && electron .",
    "electron:build": "npm run build && electron-builder",
    "backend:dev": "ts-node-dev --respawn --transpile-only src/backend/server.ts",
    "setup": "node scripts/setup-whisper.js",
    "setup:tiny": "node scripts/setup-whisper.js tiny",
    "setup:base": "node scripts/setup-whisper.js base",
    "setup:small": "node scripts/setup-whisper.js small",
    "setup:medium": "node scripts/setup-whisper.js medium",
    "setup:large": "node scripts/setup-whisper.js large",
    "test": "jest",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "video",
    "translation",
    "whisper",
    "tts",
    "ai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "msedge-tts": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "youtube-dl-exec": "^2.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "css-loader": "^6.9.1",
    "electron": "^39.2.7",
    "electron-builder": "^24.9.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
